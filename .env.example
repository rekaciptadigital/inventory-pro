# API Configuration
API_URL=https://api.example.com

# Authentication
# JWT_SECRET=your_jwt_secret_here
# JWT_EXPIRY=24h

# Database Configuration
# DB_HOST=localhost
# DB_PORT=5432
# DB_USER=postgres
# DB_PASSWORD=your_password
# DB_NAME=archery_db

# Redis Cache (Optional)
# REDIS_URL=redis://localhost:6379
